{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --quiet \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "prettier": "prettier --list-different \"**/*.ts\"",
    "prettier:fix": "prettier --write \"**/*.ts\"",
    "format": "npm run lint:fix && npm run prettier:fix",
    "prisma:introspect": "prisma introspect",
    "prisma:generate": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "DEBUG=testcontainers node --experimental-vm-modules $(yarn bin jest) --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "10.1.3",
    "@nestjs/common": "9.1.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.1",
    "@nestjs/graphql": "10.0.21",
    "@nestjs/platform-express": "9.1.1",
    "@nestjs/terminus": "9.1.1",
    "@prisma/client": "4.5.0",
    "@vendia/serverless-express": "4.10.1",
    "apollo-server-core": "3.10.3",
    "apollo-server-express": "3.10.3",
    "aws-lambda": "1.0.7",
    "aws-serverless-express": "3.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dotenv": "16.0.2",
    "graphql": "16.5.0",
    "graphql-tools": "8.2.11",
    "http": "0.0.1-security",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.1.7",
    "@nestjs/cli": "9.1.3",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.1",
    "@prisma/generator-helper": "^4.0.0",
    "@types/aws-lambda": "8.10.108",
    "@types/express": "4.17.14",
    "@types/jest": "29.1.1",
    "@types/node": "16.11.62",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "await-spawn": "^4.0.2",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "29.1.2",
    "prettier": "2.7.1",
    "prisma": "4.5.0",
    "prisma-erd-generator": "^1.2.2",
    "serverless": "3.22.0",
    "serverless-offline": "11.0.1",
    "serverless-plugin-typescript": "2.1.2",
    "serverless-ssm-fetch": "2.0.0",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "testcontainers": "^8.12.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "16"
  }
}
