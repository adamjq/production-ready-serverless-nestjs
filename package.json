{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start-offline": "serverless offline",
    "lint": "eslint --quiet \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "prettier": "prettier --list-different \"**/*.ts\"",
    "prettier:fix": "prettier --write \"**/*.ts\"",
    "format": "npm run lint:fix && npm run prettier:fix",
    "prisma:introspect": "prisma introspect",
    "prisma:generate": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npx supabase start && npx dotenv -e .env.e2e-test -- npx prisma migrate dev && npx dotenv -e .env.e2e-test -- jest --config ./test/jest-e2e.json"
  },
  "resolutions": {
    "graphql": "16.6.0"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@apollo/subgraph": "2.4.1",
    "@as-integrations/fastify": "^1.3.2",
    "@nestjs/apollo": "12.0.7",
    "@nestjs/common": "10.2.1",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.2.1",
    "@nestjs/graphql": "12.0.8",
    "@nestjs/platform-express": "10.2.1",
    "@nestjs/terminus": "10.0.1",
    "@prisma/client": "5",
    "@vendia/serverless-express": "4.10.4",
    "apollo-server-core": "3.11.0",
    "apollo-server-express": "3.10.3",
    "aws-lambda": "1.0.7",
    "aws-serverless-express": "3.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "express": "^4.17.1",
    "fastify": "^4.4.0",
    "graphql": "16.6.0",
    "graphql-tools": "8.2.11",
    "http": "0.0.1-security",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^8.14.0",
    "@nestjs/cli": "10.1.15",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.1",
    "@prisma/generator-helper": "^4.0.0",
    "@types/aws-lambda": "8.10.126",
    "@types/express": "4.17.14",
    "@types/jest": "29.5.8",
    "@types/node": "16.11.62",
    "@types/supertest": "2.0.12",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "29.7.0",
    "prettier": "2.7.1",
    "prisma": "5",
    "prisma-erd-generator": "^1.2.5",
    "serverless": "3.28.1",
    "serverless-http": "^3.2.0",
    "serverless-offline": "12.0.4",
    "serverless-ssm-fetch": "2.0.0",
    "source-map-support": "0.5.21",
    "supabase": "^1.50.9",
    "supertest": "6.2.4",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4",
    "webpack": "5.87.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "engines": {
    "node": "18"
  }
}
