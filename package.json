{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --quiet \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "prettier": "prettier --list-different \"**/*.ts\"",
    "prettier:fix": "prettier --write \"**/*.ts\"",
    "format": "npm run lint:fix && npm run prettier:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.13",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.13",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/terminus": "^8.0.4",
    "@vendia/serverless-express": "^4.5.4",
    "apollo-server-core": "^3.8.1",
    "apollo-server-express": "^3.8.1",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "graphql-tools": "^8.2.11",
    "http": "0.0.1-security",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "^8.4.5",
    "@types/aws-lambda": "^8.10.93",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "^27.5.0",
    "prettier": "2.5.1",
    "serverless": "^3.18.2",
    "serverless-offline": "^8.5.0",
    "serverless-plugin-typescript": "^2.1.1",
    "serverless-plugin-warmup": "^7.1.0",
    "serverless-plugin-warmup-ts-bridge-v2": "^0.1.8",
    "serverless-ssm-fetch": "^2.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.6",
    "ts-node": "10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
